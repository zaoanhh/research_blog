  langid = {english},
  keywords = {ambient backscatter communication (AmBC),Backscatter,ergodic rate,Interference,Internet-of-Things (IoT),NOMA,Non-orthogonal multiple access (NOMA),outage probability,Power system reliability,Probability,Signal to noise ratio,Symbiosis,symbiotic radio (SR)},
