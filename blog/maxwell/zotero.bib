@article{zhangDirectionArrivalEstimationLarge2022,
  title = {Direction-of-{{Arrival Estimation}} for {{Large Antenna Arrays With Hybrid Analog}} and {{Digital Architectures}}},
  author = {Zhang, Ruoyu and Shim, Byonghyo and Wu, Wen},
  year = {2022},
  journal = {IEEE Trans. Signal Process.},
