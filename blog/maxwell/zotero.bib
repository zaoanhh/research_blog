@incollection{xuTouchPassBehaviorirrelevantTouch2020,
  title = {{{TouchPass}}: Towards Behavior-Irrelevant on-Touch User Authentication on Smartphones Leveraging Vibrations},
  shorttitle = {{{TouchPass}}},
  booktitle = {Proceedings of the 26th {{Annual International Conference}} on {{Mobile Computing}} and {{Networking}}},
  author = {Xu, Xiang Yu and Yu, Jia Di and Chen, Ying Ying and Hua, Qin and Zhu, Yan Min and Chen, Yi-Chao and Li, Ming Lu},
  year = {2020},
