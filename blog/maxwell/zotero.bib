}

@article{wangCramerRaoBoundsField2024,
  title = {Cram{\'e}r-{{Rao Bounds}} for {{Near-Field Sensing With Extremely Large-Scale MIMO}}},
  author = {Wang, Huizhi and Xiao, Zhiqiang and Zeng, Yong},
  year = {2024},
  journal = {IEEE Trans. Signal Process.},
