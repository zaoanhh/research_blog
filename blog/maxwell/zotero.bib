@article{mudumbaiDistributedTransmitBeamforming2010,
  title = {Distributed {{Transmit Beamforming Using Feedback Control}}},
  author = {Mudumbai, Raghuraman and Hespanha, Joao and Madhow, Upamanyu and Barriac, Gwen},
  year = {2010},
  month = jan,
  journal = {IEEE Trans. Inform. Theory},
  volume = {56},
